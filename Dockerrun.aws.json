{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "customer-api",
      "image": "devgerman/customer-api:04.10.2024.16.10.01",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}